{
    "expo": {
        "name": "Note Buddy",
        "slug": "NoteBuddy",
        "version": "0.0.1",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#1b1b1f"
        },
        "ios": {
            "supportsTablet": true,
            "userInterfaceStyle": "automatic"
        },
        "android": {
            "userInterfaceStyle": "automatic",
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#1b1b1f"
            },
            "permissions": ["android.permission.RECORD_AUDIO"],
            "package": "com.benjipine.NoteBuddy"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-build-properties",
                {
                    "ios": {
                        "newArchEnabled": true
                    },
                    "android": {
                        "newArchEnabled": true
                    }
                }
            ],
            "expo-font"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "0beb6b84-6445-443b-87a9-5eddd64eba8c"
            }
        }
    }
}
